{
  "name": "tdeekens",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "dev": "(export PATH=$(npm bin):$PATH; trap 'jobs -pr | xargs kill' EXIT; TAILWIND_MODE=watch postcss ./styles/tailwind.css -o ./styles/globals.css --watch </dev/zero & next dev)",
    "build": "next build",
    "format": "prettier --write '**/*.{js, ts, tsx}'",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "css:dev": "TAILWIND_MODE=watch postcss ./styles/tailwind.css -o ./styles/globals.css --watch",
    "css:build": "postcss ./styles/tailwind.css -o ./styles/globals.css",
    "prepare": "husky install"
  },
  "dependencies": {
    "@u-wave/react-youtube": "^0.7.2",
    "gray-matter": "4.0.3",
    "next": "12.0.7",
    "next-mdx-remote": "3.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "8.11.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "autoprefixer": "10.4.0",
    "babel-jest": "27.4.5",
    "eslint": "<8.0.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "jest-dom": "4.0.0",
    "lint-staged": "12.1.2",
    "node-mocks-http": "1.11.0",
    "postcss": "8.4.5",
    "postcss-cli": "9.1.0",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.2",
    "ts-jest": "27.1.1",
    "ts-loader": "9.2.6",
    "tsc-files": "1.1.3",
    "typescript": "4.5.4"
  }
}
